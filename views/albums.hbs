<h2>Albums</h2>

<div class="query-results-container">
    {{#each albums}}
        <div class="query-results-box">
            <h5>{{name}}</h5>
            <img src="{{images.[0].url}}" alt="Artist">
            <a href="/album/tracks/{{id}}">{{name}} Tracks</a>
        </div>
    {{/each}}
</div>
